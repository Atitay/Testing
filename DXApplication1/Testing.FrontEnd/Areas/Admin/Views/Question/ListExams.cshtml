@using Testing.Models
@model Testing.Models.Question
@{
    ViewData["Title"] = "ListExams";
}

<div class="row">
    <div class="col-sm pb-4">
        <br />
        <div class="text-secondary p-1" style="background-color:#efdfdf">
            <h5>List Exam of Question :&nbsp;&nbsp;@Model.QuestionString</h5>
        </div>
        <br />
        <partial name="_lstExams" />
        <br />

        @(Html.DevExtreme().Button()
                .Text("Back")
                .Type(ButtonType.Back)
                .StylingMode(ButtonStylingMode.Contained)
                .OnClick("BacktoIndex")
        )
    </div>
    
</div>

<script>
    function BacktoIndex() {
       window.location.href = "/Admin/Questions/Index";
    }

</script>